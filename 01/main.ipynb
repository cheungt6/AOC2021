   "source": [
    "prev = None\n",
    "curr = None\n",
    "count = 0\n",
    "with open('input', 'r') as f:\n",
    "    line = f.readline()\n",
    "    while line:\n",
    "        curr = int(line)\n",
    "        if prev is not None:\n",
    "            if curr > prev:\n",
    "                count += 1\n",
    "        prev = curr\n",
    "        line = f.readline()\n",
    "count"
   ]
  }
